        (ff.add |Pythagoras[0].a2| |Pythagoras[0].b2|)), 
    (= |Square[0].out| (ff.mul |Square[0].in| |Square[0].in|)), 
    (= |Square[1].out| (ff.mul |Square[1].in| |Square[1].in|))
